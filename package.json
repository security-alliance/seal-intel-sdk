{
    "name": "@security-alliance/seal-isac-sdk",
    "version": "0.1.2",
    "description": "SEAL-ISAC SDK",
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "exports": {
        ".": "./dist/index.js",
        "./web-content": "./dist/web-content/index.js"
    },
    "typesVersions": {
        "*": {
            ".": [
                "dist/index.d.ts"
            ],
            "./web-content": [
                "dist/web-content/index.d.ts"
            ]
        }
    },
    "scripts": {
        "build": "rm -rf dist; tsc -p tsconfig.release.json",
        "test": "mocha",
        "prepublishOnly": "npm run build",
        "prettier:check": "npx prettier -c \"**/*.{js,ts,md,json,yml,yaml}\"",
        "prettier:fix": "npx prettier -w \"**/*.{js,ts,md,json,yml,yaml}\""
    },
    "bin": {
        "seal-isac": "dist/cmd/index.js"
    },
    "keywords": [
        "seal-isac",
        "sdk",
        "security-alliance"
    ],
    "author": "Security Alliance",
    "license": "UNLICENSED",
    "files": [
        "dist/"
    ],
    "dependencies": {
        "@apollo/client": "^3.10.4",
        "@security-alliance/opencti-client": "0.1.2",
        "@security-alliance/stix": "^0.1.1",
        "apollo-upload-client": "^18.0.1",
        "graphql": "^16.8.1",
        "tldts": "^6.1.50",
        "yargs": "^17.7.2"
    },
    "devDependencies": {
        "@types/apollo-upload-client": "^18.0.0",
        "@types/node": "^20.12.7",
        "@types/yargs": "^17.0.33",
        "mocha": "^10.7.3",
        "prettier": "^3.3.3",
        "tsx": "^4.19.1",
        "typescript": "^5.4.5"
    }
}
